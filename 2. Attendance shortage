total_classes = float(input("Enter total classes: "))
class_attended = float(input("Enter no.of classes attended: "))

attendance_percentage = (class_attended/total_classes)*100
per = (total_classes*3/4) - class_attended
print(f"Attendance percentage: {attendance_percentage}%")

if attendance_percentage < 75:
    print("Not Eligible")
    additional_classes = total_classes - class_attended 
    print(f"Additional classes required: {per}")
else:
    print("Eligible")

